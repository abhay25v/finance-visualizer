{
  "version": 2,
  "name": "finance-visualizer",
  "builds": [
    {
      "src": "next.config.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "NEXT_PUBLIC_APP_URL": "@next_public_app_url"
  }
}
